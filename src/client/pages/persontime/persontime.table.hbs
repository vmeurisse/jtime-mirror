<table class="jtime-persontime-table">
	{{#each weeks}}
		<tr>
			{{#each this}}
				<td {{#if invalid}}class="invalid"{{/if}}>
					{{#if date}}<h4>{{date}}</h4>{{/if}}
					{{#each works}}
						<span class="item color{{color}}" data-index="{{index}}" style="line-height:{{height}}px; font-size:{{fontSize}}px" title="{{title}}">{{CR}}</span>
					{{/each}}
				</td>
			{{/each}}
		</tr>
	{{/each}}
</table>